@using EntityLayer.Concrete
@model List<About>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/CoreLayout.cshtml";
}

<html lang="zxx">

<body>
	<!--/main-->
	<section class="main-content-w3layouts-agileits">
		<div class="container">
			<h3 class="tittle">Hakkımda</h3>
			<div class="row inner-sec">
				<!--left-->
				<div class="col-lg-8 left-blog-info-w3layouts-agileits text-left">
					<div class="blog-grid-top">

						<div class="blog_info_left_grid">
							<a href="single.html">
								<img src="~/coreblogtema/web/images/b5.jpg" class="img-fluid" alt="">
							</a>
						</div>
						<h3>
							Misyon & Vizyon
						</h3>
							@foreach(var item in Model)
							{
							<p>@item.Details1</p>
							<br />
								
							}

					</div>
				</div>
				<!--//left-->
				<!--right-->
				<aside class="col-lg-4 agileits-w3ls-right-blog-con text-right">
					<div class="right-blog-info text-left">
						<div class="tech-btm">
							<img src="~/coreblogtema/web/images/banner1.jpg" class="img-fluid" alt="">
						</div>
						
						@await Html.PartialAsync("~/Views/Newsletter/SubscribeNewsletter.cshtml")
						
						@await Html.PartialAsync("~/Views/About/SocialMedia.cshtml")

						@await Component.InvokeAsync("VCBlogListByWriter")
					</div>
				</aside>
				<!--//right-->
			</div>
		</div>
	</section>
</body>

</html>